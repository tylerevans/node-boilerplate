div.navbar.navbar-default.navbar-fixed-top(role='navigation')
	div.container
		div.navbar-header
			button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
				span.icon-bar
				span.icon-bar
				span.icon-bar
			a.navbar-brand(href="#")
				| Program Name
		div.collapse.navbar-collapse
			ul.nav.navbar-nav
				mixin header-item(name, url)
					if name.toLowerCase() == nav
						li.active
							a(href=url)= name
					else
						li
							a(href=url)= name
				+header-item('Home', '/')
				+header-item('About', '/about')
